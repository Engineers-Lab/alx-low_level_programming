#include "main.h"
#include <stdio.h>
/**
 *print_array - print a nomber of elements from an array
 *@a: array
 *@n: nomber of elements
 *
 *Return: nothing
 */
void print_array(int *a, int n)
{
	int i;
	int j;

	for (i = n; i < j; i++)
	{
		if (i < j - 1)
			printf("%d, ", a[i]);
		else
			printf("%d", a[i]);
	}
}
